#include <iostream>
#include <string>
using namespace std;

int main() {
string email;
cout << "Introduce tu correo electrónico: ";
cin >> email;
size_t pos = email.find('@');
string nombre = email.substr(0, pos);
string nuevo_email = nombre + "@ceu.es";
cout<<"Tu nuevo correo electrónico es: "<<nuevo_email <<endl;
  return 0;
}
